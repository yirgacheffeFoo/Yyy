<view><view class="musicBox"><block wx:for="{{musicSelfList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['getMusicUrl',['$0','$1','$2'],[[['musicSelfList','',index,'id']],[['musicSelfList','',index,'name']],[['musicSelfList','',index,'al.picUrl']]]]]]]}}" class="musicItem" bindtap="__e"><view><text class="musicIndex">{{index+1+'.'}}</text><text class="musicName">{{''+item.name}}</text></view><view class="singer">{{"----"+item.ar[0].name}}</view></view></block></view><view class="tabbar"><image class="musicImg" src="{{musicUrl}}" mode></image><text>{{musicName}}</text><image class="icon" src="{{icon[iconAction]}}" mode data-event-opts="{{[['tap',[['setIcon',['$event']]]]]}}" bindtap="__e"></image></view></view>