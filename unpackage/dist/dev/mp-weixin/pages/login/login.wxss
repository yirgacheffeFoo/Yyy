.input {
	width: 250rpx;
	height: 60rpx;
	padding-left: 10px;
}
.hand image {
	width: 180rpx;
	height: 180rpx;
}
.hand {
	text-align: center;
}
.img {
	width: 40rpx;
	height: 40rpx;
}
.id,
.psd {
	border: 1px solid #000000;
	border-radius: 30px;
	display: flex;
	width: 500rpx;
	height: 100rpx;
	align-items: center;
	padding-left: 15px;
}
button {
	background-color: #FFFFFF;
}
.btn image {
	width: 120rpx;
	height: 120rpx;
}
.login {
	display: flex;
	height: 500rpx;
	flex-direction: column;
	align-items: center;
	justify-content: space-around;
	margin-top: 50px;
}
.box {
	padding-top: 80px;
	height: 100%;
}
page {
	height: 100%;
}
.other {
	display: flex;
	justify-content: space-around;
	align-items: center;
	margin-top: 90px;
}
.line {
	width: 150rpx;
	border-bottom: #C0C0C0 2px solid;
}
.other-text {
	font-size: 16px;
}
.icon {
	display: flex;
	justify-content: center;
}
.icon image {
	width: 60rpx;
	height: 60rpx;
	margin-right: 10px;
	margin-left: 10px;
	margin-top: 20px;
}

