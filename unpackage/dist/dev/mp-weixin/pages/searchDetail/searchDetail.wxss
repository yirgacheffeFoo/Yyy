.search-ico image {
	width: 80rpx;
	height: 80rpx;
}
.search-text {
	font-size: 14px;
	width: 650rpx;
	padding-left: 20px;
}
.search {
	display: flex;
	flex-direction: row;
	margin: 100rpx 50rpx 50rpx;
	border: 1px solid #808080;
	align-items: center;
	border-radius: 10px;
}
.search-ico {
	display: flex;
	flex-direction: column;
	justify-content: center;
	padding: 0rpx 10rpx;
}
.musicImg {
	width: 90rpx;
	height: 90rpx;
	border-radius: 100rpx;
	margin-left: 20px;
	margin-right: 10px;
}
.tabbar {
	border-top: #C8C7CC 1px solid;
	height: 100rpx;
	display: flex;
	align-items: center;
	position: fixed;
	bottom: 0;
	width: 100%;
	background-color: #FFFFFF
}
.tabbar text {
	font-size: 18px
}
.icon {
	width: 60rpx;
	height: 60rpx;
	margin-left: auto;
	margin-right: 30px;
}

