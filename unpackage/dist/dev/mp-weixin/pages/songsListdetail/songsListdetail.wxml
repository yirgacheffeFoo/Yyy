<view><view class="songBox-one"><view><image class="songsImg" src="{{songsListDetail.coverImgUrl}}" mode></image></view><text class="songsText">{{songsListDetail.name}}</text></view><view class="musicBox"><block wx:for="{{musicList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['getMusicUrl',['$0','$1','$2'],[[['musicList','',index,'id']],[['musicList','',index,'name']],[['musicList','',index,'al.picUrl']]]]]]]}}" class="musicItem" bindtap="__e"><text class="musicIndex">{{index+1+"."}}</text><text class="musicName">{{''+item.name}}</text></view></block></view><view class="tabbar"><image class="musicImg" src="{{musicUrl}}" mode></image><text>{{musicName}}</text><image class="icon" src="{{icon[iconAction]}}" mode data-event-opts="{{[['tap',[['setIcon',['$event']]]]]}}" bindtap="__e"></image></view></view>