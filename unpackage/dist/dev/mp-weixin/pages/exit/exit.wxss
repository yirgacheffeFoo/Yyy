.hand image {
	width: 200rpx;
	height: 200rpx;
}
.hand {
	text-align: center;
}
.id,
.psd {
	border: 1px solid #000000;
	border-radius: 30px;
	display: flex;
	width: 500rpx;
	height: 120rpx;
	align-items: center;
	justify-content: center;
	font-size: 18px;
	font-weight: 500;
}
.id {
	background-color: #c1e2d7;
	color: #FFFFFF;
}
.psd {
	background-color: #FFFFFF;
}
.login {
	display: flex;
	height: 400rpx;
	flex-direction: column;
	align-items: center;
	justify-content: space-around;
	margin-top: 80px;
}
.box {
	padding-top: 80px;
	height: 100%;
}
page {
	height: 100%;
}
.other {
	display: flex;
	justify-content: space-around;
	align-items: center;
	margin-top: 90px;
}
.line {
	width: 200rpx;
	border-bottom: #C0C0C0 2px solid;
}
.other-text {
	font-size: 16px;
}
.icon {
	display: flex;
	justify-content: center;
}
.icon image {
	width: 100rpx;
	height: 100rpx;
	margin-right: 10px;
	margin-left: 10px;
	margin-top: 20px;
}

